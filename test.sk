module Main {

enum Bool {
    True,
    False
}

enum Option[T] {
    Some(T),
    None
}

class Container[T] {
    item: Option[T]

    fn new() -> Container[T] {
        Container(None)
    }

    fn push(self, item: T) -> Container[T] {
        Container(Some(item))
    }
}

fn main() {
    let mut a : Container[Bool] = Container.new();
    a.push(True);
    a.item;
}

}