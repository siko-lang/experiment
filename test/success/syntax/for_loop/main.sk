module Main {

fn main() {
    let items = [1, 2, 3, 4, 5];
    let mut sum = 0;
    // for item in items {
    //     sum = sum + item;
    // }
    let mut iter = items.intoIterator();
    loop {
        match iter.next() {
            Some(item) -> {
                sum = sum + item;
            }
            None -> {
                break;
            }
        }
    }
}

}