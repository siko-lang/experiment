module Main {

fn main() {
    let r = loop a = 1 {
        if a.lessThan(10) {
            continue a.add(1);
        } else {
            break a;
        }
    };
    assert(r.eq(10));
    let r = loop a = 1 {
        if a.lessThan(10) {
            a.clone().add(1)
        } else {
            break a;
        }
    };
    assert(r.eq(10));
}

}